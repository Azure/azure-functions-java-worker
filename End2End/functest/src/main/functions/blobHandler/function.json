{
    "scriptFile": "../../../../target/azure-functions-EndToEndTests-1.0-SNAPSHOT.jar",
    "entryPoint": "com.microsoft.azure.functions.endtoend.Blob.blobHandler",
    "bindings": [
        {
            "type": "queueTrigger",
            "name": "myQueueItem",
            "queueName": "myqueueitem",
            "connection": "AzureWebJobsStorage",            
            "direction": "in"
          },
          {
            "type": "blob",
            "name": "myInputBlob",      
            "path": "myblob/{queueTrigger}",
            "connection": "AzureWebJobsStorage",
            "direction": "in"
          },
          {
            "type": "blob",
            "name": "myOutputBlob",
            "path": "myblob/{queueTrigger}-Copy",
            "connection": "AzureWebJobsStorage",
            "direction": "out"
          }
    ],
    "disabled": false
}

